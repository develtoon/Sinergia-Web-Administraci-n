.c-file
    &-card--1
        height 200px
        width 100%
        //min-width 160px
        box-shadow: 0px 2px 6px 0px rgba(#0F296A, 0.5);
        border-radius: 18px;
    &-card--2
        height calc(100vh - 250px)
        width 100%
        box-shadow: 0px 2px 6px 0px rgba(#0F296A, 0.5);
        border-radius: 18px;
    &-card--responsive
        background-position: 50%;
        background-size: contain;
        background-repeat: no-repeat;
